    "class Dog:\n",
    "    species=\"mammals\"       \n",
    "    is_hungry=True\n",
    "    def __init__(self, name, age):\n",
    "        self.name = name\n",
    "        self.age = age\n",
    "\n",
    "    def description(self):\n",
    "        print(\"{} is {} years old\".format(self.name, self.age))\n",
    "    \n",
    "    def eat(self):\n",
    "        self.is_hungry = False\n",
    "    \n",
    "def branco(cani):\n",
    "    print(\"I have \", len(cani), \" dogs\")\n",
    "    conta=False\n",
    "    for i in range(len(cani)):\n",
    "        if cani[i].is_hungry==True:\n",
    "            conta=True\n",
    "    if conta:\n",
    "        print(\"My dogs are hungry.\")\n",
    "    else:\n",
    "        print(\"My dogs are not hungry\")\n",
    "\n",
    "\n",
    "\n",
    "can=[]\n",
    "buddy=Dog(\"buddy\",12)\n",
    "puppy=Dog(\"tobia\",10)\n",
    "\n",
    "can.append(buddy)\n",
    "can.append(puppy)\n",
    "\n",
    "branco(can)\n",
    "\n",
    "buddy.description()\n",
    "puppy.description()\n",
    "buddy.eat()\n",
    "puppy.eat()\n",
    "\n",
    "branco(can)"
